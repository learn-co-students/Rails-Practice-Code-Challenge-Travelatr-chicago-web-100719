<%if flash[:errors]%>
  <%flash[:errors].each do |error|%>
    <%=error%>
    <br>
  <%end%>
<%end%>

<%=form_for @blogger do |f|%>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <br>
  <%=f.label :age%>
  <%=f.number_field :age%>
  <br>
  <%=f.label :bio%>
  <%=f.text_area :bio%>
  <br>
  <%=f.submit%>
<%end%>